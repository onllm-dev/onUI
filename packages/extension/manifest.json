{
  "manifest_version": 3,
  "name": "onUI",
  "version": "1.0.5",
  "key": "",
  "description": "Annotate UI elements for AI agents - no code integration required",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "default_title": "onUI - Annotate UI Elements",
    "default_popup": "src/popup/index.html"
  },
  "background": {
    "service_worker": "src/background/index.ts",
    "type": "module"
  },
  "permissions": [
    "activeTab",
    "scripting",
    "storage",
    "nativeMessaging",
    "alarms"
  ]
}
